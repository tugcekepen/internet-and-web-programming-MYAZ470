<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head') %> <!-- This line includes the head.ejs file -->

    <body>
        <h1>
            <%= contenttitle %>
        </h1>

        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Surname</th>
                    <th scope="col">Email</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach((element, index) => { %>
                    <tr>
                        <th scope="row">
                            <%= index+1 %>
                        </th>
                        <td>
                            <%= element.name %>
                        </td>
                        <td>
                            <%= element.surname %>
                        </td>
                        <td>
                            <%= element.email %>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        </table>

        <% data.forEach(element=> { %>



            <% }) %>
    </body>

</html>